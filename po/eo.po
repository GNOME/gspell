msgstr "Kontroli _vorton"

#: gspell/resources/checker-dialog.ui:120
msgid "_Suggestions:"
msgstr "_Proponoj:"

#: gspell/resources/checker-dialog.ui:133
msgid "_Ignore"
msgstr "_Ignori"

#: gspell/resources/checker-dialog.ui:146
msgid "Ignore _All"
