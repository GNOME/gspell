#: gspell/resources/checker-dialog.ui:139
msgid "Ignore _All"
msgstr "Bỏ qu_a tất cả"

#: gspell/resources/checker-dialog.ui:153
msgid "Cha_nge"
msgstr "Thay đổ_i"

#: gspell/resources/checker-dialog.ui:169
msgid "Change A_ll"
msgstr "Thay đổi tất _cả"

#: gspell/resources/checker-dialog.ui:187
